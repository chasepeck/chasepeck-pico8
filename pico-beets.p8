				if not(balls[self].bounce)then 
					balls[self].hit=true
					balls[self].col=1
				else 
					if(balls[self].hits==1)then 
						balls[self].hit=true
						balls[self].col=1
					else balls[self].hits-=1 end
				end
				streak+=1
				score+=2*streak
				sfx(61,3)
			end
			
			if(balls[self].x<=5 and 
			balls[self].y>=pady-5 
			and balls[self].y<=pady+5 
			and balls[self].x>=0
			and not balls[self].hit)then
				if(balls[self].x<=3)balls[self].x+=6
				paddle_hit()
			end
			
			//gravity
			if(balls[self].bounce)balls[self].xvel-=balls[self].gravity
			
			//off-screen & miss
			if(balls[self].x>130 or balls[self].x<-2)then
				balls[self].xvel=0
				balls[self].yvel=0
				if(balls[self].hit==false and balls[self].miss==false and balls[self].x<-2)then
					sfx(59,3)
					score-=5
					streak=0
					balls[self].miss=true
				end
			end
			
			//bouncing on walls
			if(balls[self].y>=126 or balls[self].y<=2)then
				balls[self].yvel=-balls[self].yvel
			end
			
			balls[self].x+=balls[self].xvel
			balls[self].y+=balls[self].yvel
		
		end,
		
		x=_x,
		y=_y,
		xvel=_xvel,
		yvel=_yvel,
		col=_col,
		hit=false,
		miss=false,
		bounce=_bounce,
		gravity=_gravity,
		hits=_hits,
		
	})
end
