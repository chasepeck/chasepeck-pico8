-- misc functions

function rand_table(table)
	return table[ceil(rnd(#table))]
end

function palette(table)
	local n=1
	for i=0x5f00,0x5f0f do
		poke(i,table[n])
		n+=1
	end
end

function gtlt(v,a,b)
	if(v>a and v<b)return true
	return false
end
