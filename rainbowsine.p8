function _draw()
	cls()
	pval=0
	for l=1,8 do
	for i=-1,137 do
		val=sin(i/l/10-time())*20
		line(
			i-l,
			64-pval+l*2,
			i-l+1,
			64-val+l*2,
			7+l
		)
		pval=val
	end
	end
end
