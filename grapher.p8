pico-8 cartridge // http://www.pico-8.com
version 41
__lua__
function graph(x)
	return x^2
end

function _draw()
	cls()
	line(64,0,64,128,1)
	line(0,64,128,64)
	
	pval=graph(-65)
	for i=-64,64 do
		val=graph(i)
		line(i+63,64-pval,i+64,64-val,8)
		pval=val
	end
end
