version 32
__lua__
function _init()
	alphabet={
	"a",
	"b",
	"c",
	"d",
	"e",
	"f",
	"g"}
	for i=0,10 do
		print("\a"..alphabet[loopover(3+i*2,#alphabet)].."1")
	end
end

function loopover(v,l)
	if(v>l) then
		return abs(v-l)
	else return v end
end
